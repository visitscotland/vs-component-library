    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "speed-measure-webpack-plugin": "^1.5.0",
    "storybook": "^7.3.2",
    "style-loader": "^3.3.2",
    "svgo-loader": "^4.0.0",
    "time-analytics-webpack-plugin": "^0.1.20",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "vite-plugin-turbosnap": "^1.0.2",
    "vue-chart-3": "^3.1.8",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-loader": "^17.1.0",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2",
    "webpack-manifest-plugin": "^5.0.0"
  },
  "dependencies": {
    "@mapbox/geojson-extent": "^1.0.1",
    "@storybook/addon-a11y": "^7.0.18",
    "@unhead/vue": "^1.1.27",
    "@visitscotland/product-search": "^0.1.9",
    "@vueform/multiselect": "^2.6.2",
    "@vuelidate/core": "^2.0.2",
    "@vuelidate/validators": "^2.0.2",
    "axios": "^1.4.0",
    "bootstrap-vue-next": "^0.9.20",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "glob": "^8.1.0",
    "lodash": "^4.17.21",
    "mapbox-gl": "^1.11.1",
    "mitt": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "optimist": "^0.6.1",
    "pinia": "^2.1.3",
    "require-context.macro": "^1.2.2",
    "slugify": "^1.6.6",
    "standard-version": "^9.5.0",
    "storybook-addon-pseudo-states": "^2.0.1",
    "theo": "^8.1.5",
