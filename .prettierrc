{
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "semi": true,
    "requirePragma": false,
    "insertPragma": false,
    "useTabs": false,
    "tabWidth": 4,
    "arrowParens": "always",
    "proseWrap": "preserve",
    "overrides": [
        {
            "files": "*.scss",
            "options": {
                "parser": "scss"
            }
        }
    ]
}
